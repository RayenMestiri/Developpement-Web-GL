    "install:ts": "cd 02-TypeScript && npm install",
    "install:angular": "cd 03-Angular && npm install",
    "install:nextjs": "cd 04-NextJS && npm install",
    "test": "npm run test:js && npm run test:ts && npm run test:angular && npm run test:nextjs",
    "test:js": "cd 01-JavaScript-avance && npm test",
    "test:ts": "cd 02-TypeScript && npm test",
    "test:angular": "cd 03-Angular && npm test",
    "test:nextjs": "cd 04-NextJS && npm test",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:angular && npm run lint:nextjs",
    "lint:js": "cd 01-JavaScript-avance && npm run lint",
    "lint:ts": "cd 02-TypeScript && npm run lint",
